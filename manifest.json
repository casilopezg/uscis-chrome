{
  "manifest_version": 2,

  "name": "USCIS Scraper",
  "description": "Scrape data from USCIS",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://egov.uscis.gov/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
